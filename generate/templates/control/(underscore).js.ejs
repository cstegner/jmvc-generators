steal(
    'can',
    './init.mustache',
function(can, initView){
    /**
     * @class <%= module %>
	 * @alias <%= Alias %>   
     */
    return can.Control(
	/** @Static */
	{
		defaults : {}
	},
	/** @Prototype */
	{
		init : function(){
			this.element.html(initView({
				message: "Hello World from <%= Alias %>"
			}));
		}
	});
});